<Window x:Class="LocalizationGenerator.ExtractXAMLelements"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ExtractXAMLelements" Height="400" Width="600">
    <Grid>
        <Button x:Name="txtOpenFile" Content="Open XAML file..." Click="txtOpenFile_Click_1" HorizontalAlignment="Left" Margin="10,47,0,0" VerticalAlignment="Top" Width="134"/>
        <Button x:Name="btnCopyToClipboard" Content="Copy Result To Clipboard" HorizontalAlignment="Right" Margin="421,47,0,0" VerticalAlignment="Top" Width="161"/>
        <TextBox x:Name="txtResult" Height="296" Margin="0,74,0,0" TextWrapping="Wrap" Text="Result"/>
        <TextBlock Margin="10,10,0,0" TextWrapping="Wrap" Text="Open .xaml file. This file will be parsed and applicable elements that have a Name and Text/Content attribute will be parsed into Fast#" VerticalAlignment="Top"/>

    </Grid>
</Window>
